#include<stdio.h>
void main()
{
int marks;
printf("Enter your marks ");
scanf"%d",&marks);
printf("\n So , grade will be :\n\n");
if(marks<0||marks>100)
{
printf("Wrong Entry");
}
else if(marks>=0||marks<40)
{
printf("\nGrade F ");
}
else if(marks>=40||marks<=54)
{
printf("\nGrade D ");
}
else if(marks>=55||marks<=69)
{
printf("\nGrade C ");
}
else if(marks>=70||marks<=84)
{
printf("\nGrade B ");
}
else
{printf("\nGrade A ");
}
}
